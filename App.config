<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <runtime>
        <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
            <!-- Assembly binding redirects if needed -->
        </assemblyBinding>
    </runtime>
    <startup>
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
    <appSettings>
        <!-- Application settings -->
        <add key="AutoSaveInterval" value="300000" /> <!-- 5 minutes in milliseconds -->
        <add key="MaxRecentFiles" value="10" />
        <add key="ThemeName" value="Dark" />
    </appSettings>
</configuration>
